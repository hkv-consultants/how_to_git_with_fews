	<import>
		<general>
			<importType>generalCSV</importType>
			<folder>$IMPORT_FOLDER$/Keller</folder>
			<fileNamePatternFilter>Export*.CSV</fileNamePatternFilter>
			<failedFolder>$IMPORT_FAILED_FOLDER$/Keller</failedFolder>
			<backupFolder>$IMPORT_BACKUP_FOLDER$/Keller</backupFolder>
			<table>
				<!--"locatie","tijdstip","waarde","commentaar"
	  		"B45C0949","20070101 12:00","-999.00",
		-->
				<dateTimeColumn name="tijdstip" pattern="yyyyMMdd HH:mm"/>
				<locationColumn name="locatie"/>
				<valueColumn name="waarde" unit="SI"/>
				<commentColumn name="commentaar"/>
			</table>
			<unitConversionsId>ImportUnitConversions</unitConversionsId>
			<missingValue>-999.00</missingValue>
			<importTimeZone>
				<!--				<timeZoneName>MET</timeZoneName>-->
				<timeZoneOffset>+01:00</timeZoneOffset>
			</importTimeZone>
			<dataFeedId>Ultimo handmetingen</dataFeedId>
			<convertDatum>false</convertDatum>
			<reportChangedValues>true</reportChangedValues>
		</general>
		<timeSeriesSet>
			<moduleInstanceId>HandmatigeInvoer</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>G.meting.hand</parameterId>
			<locationSetId>grondwater</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="nonequidistant"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</import>
